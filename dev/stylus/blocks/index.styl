.catalog
  pos relative
  z-index 1
  bgc white
  sp()
  pt 151px
  pb 107px
  display flex
  justify-content space-between
  align-items flex-start
  @media $lDesktopMedium
    pt 148px
    pb 107px
  @media $LandscapeMax
    flex-direction column
    pt 98px
    pb 68px
  @media $PortraitMax
    pb 0
  &__main,
  &__contacts
    w calc(40% - 25px)
    @media $desktopMin
      w calc(50% - 15px)
    @media $LandscapeMax
      w 100%
  &__hrefs
    &-row
      display flex
      justify-content space-between
      @media $desktopMin
        justify-content flex-start
        flex-wrap wrap
      @media $PortraitMax
        flex-direction column
    &-col
      w calc(50% - 25px)
      @media $desktopMin
        w auto
        & + &
          ml 30px
          @media $PortraitMax
            ml 0
            mt 20px
  &__href
    color #3b3b3b
    fontify 19px 400 30px
    border-bottom 1px solid
    transition .7s border
    @media $lDesktopMedium
      fontify 14px 400 23px
    &:hover
      border-color transparent
    &-info
      color #3b3b3b
      opacity .65
      fontify 15px 400 15px
      mt 16px
      @media $lDesktopMedium
        fontify 12px 400 15px
  &__contacts
    mt -40px
    background-color #f3f5f8
    padding 42px 56px 51px
    @media $lDesktopMedium
      padding 36px 38px 46px
    @media $LandscapeMax
      mt 40px
    @media $PortraitMax
      ml -40px
      w calc(100% + 80px)
      padding 38px 40px
    &-title
      color #3b3b3b
      font-family $text
      fontify 25px 500 29px
      mb 23px
      @media $lDesktopMedium
        fontify 19px 500 23px
        mb 16px
    &-mail
      color #3b3b3b
      fontify 19px 400 30px
      border-bottom 1px solid
      transition .7s border
      @media $lDesktopMedium
        fontify 14px 400 23px
      &:hover
        border-color transparent