.top
  pos fixed
  display flex
  align-items flex-end
  justify-content space-between
  t 0
  l 0
  w 100%
  h 100vh
  sp()
  @media $desktopMin
    flex-direction column
    align-items flex-start
    justify-content flex-end
  &__bg
    pos absolute
    z-index -2
    t 0
    l 0
    min-height 100%
    min-width 100%
    @media $desktopMin
      w calc(100% + 40px)
  &:before
    content ""
    pos absolute
    z-index -1
    t 0
    l 0
    w 100%
    h 100%
    background url('../i/header-mask.png') no-repeat center
    background-size cover
    @media $desktopMin
      background-position center left
  &__tonext
    pos absolute
    l 0
    b 62px
    size 35px 23px
    cursor pointer
    opacity .5
    background url("data:image/png;base64,") no-repeat center
    transition .7s opacity
    &:hover
      opacity 1
    @media $desktopMin
      dn()
  &__about
    pos relative
    w calc(50% - 25px)
    pb 159px
    @media $lDesktopMedium
      pb 151px
    @media $desktopMin
      pb 0
      w 100%
      mb 30px
    &-place
      pos relative
      font-family $text
      fontify 56px 500 58px
      mb 22px
      @media $lDesktopMedium
        fontify 40px 500 40px
        mb 24px
      @media $desktopMedium
        fontify 30px 500 30px
      @media $PortraitMin
        fontify 22px 500 22px
      &:before
        content ""
        pos absolute
        t -35px
        l 0
        size 90px 7px
        background-color $pink
        @media $PortraitMin
          t -25px
    &-name
      font-family $comp
      text-transform uppercase
      fontify 103px 500 108px
      max-width 500px
      @media $lDesktopMedium
        fontify 72px 500 89px
      @media $desktopMedium
        fontify 52px 500 59px
      @media $PortraitMin
        fontify 36px 500 40px
  &__info
    w calc(50% - 25px)
    bgc $dblue
    padding 54px 50px
    @media $lDesktopMedium
      padding 32px 40px 43px
    @media $desktopMin
      bgc transparent
      padding 0
      pb 40px
      w 100%
    &-text
      fontify 19px 400 30px
      mb 41px
      @media $lDesktopMedium
        fontify 14px 400 23px
        mb 35px
      @media $desktopMin
        dn()
.program
  bgc $blue
  pos relative
  z-index 1
  sp()
  pt 152px
  pb 100px
  @media $lDesktopMedium
    pt 152px
    pb 152px
  @media $desktopMin
    pb 112px
  &__content
    pos relative
    display flex
    justify-content space-between
    @media $desktopMin
      flex-direction column
      justify-content flex-start
      align-items flex-start
    &-main
      w calc(60% - 25px)
      @media $desktopMin
        w 100%
    &-aside
      w calc(40% - 25px)
      pt 146px
      @media $lDesktopMedium
        pt 60px
      @media $desktopMin
        w 100%
        pt 160px
  &__list
    display flex
    flex-direction column
    align-items stretch
    pos relative
    mb 50px
    @media $lDesktopMedium
      mb 41px
    &:before
      content ""
      position absolute
      z-index -1
      l 224px
      t -90px
      w calc(100% + 35px)
      h calc(100% + 200px)
      bgc $dblue
      @media $lDesktopMedium
        l 176px
        w 100%
        t -60px
      @media $LandscapeMin
        dn()
  &__item
    display flex
    align-items flex-start
    padding 27px 0 21px
    border-bottom 1px solid alpha(white, .15)
    transition .7s background
    @media $lDesktopMedium
      padding 15px 0
    @media $LandscapeMin
      flex-direction column
    &:hover
      bgc alpha(#06305d, .5)
      @media $LandscapeMin
        bgc transparent
    &-date
      w 224px
      flex-shrink 0
      text-align center
      font-family $text
      fontify 24px 500 26px
      text-transform uppercase
      @media $lDesktopMedium
        w 175px
        fontify 19px 500 22px
      @media $LandscapeMin
        w 100%
        text-align left
        mb 10px
    &-title
      w calc(100% - 224px)
      padding 0 49px
      fontify 19px 400 30px
      mt -2px
      @media $lDesktopMedium
        w calc(100% - 175px)
        fontify 14px 400 23px
        padding 0 39px
      @media $LandscapeMin
        w 100%
        padding 0
      span
        font-size 15px
        opacity .65
        pr 8px
        @media $lDesktopMedium
          font-size 12px
  &__btns
    display flex
    align-items flex-start
    @media $LandscapeMin
      flex-direction column
  &__btn
    &_download
      pos relative
      border-radius 36px
      font-family $text
      color white
      fontify 27px 500 68px
      padding 0 47px 0 79px
      border 2px solid alpha(white, .5)
      transition .7s border
      @media $lDesktopMedium
        padding 0 32px 0 51px
        fontify 19px 500 46px
      &:hover
        border-color white
      &:before
        content ""
        pos absolute
        l 48px
        t 18px
        size 21px 30px
        background url("data:image/png;base64,") no-repeat center
        @media $lDesktopMedium
          l 29px
          t 15px
          size 13px 17px
          background-size cover
    &-reg
      ml 30px
      @media $LandscapeMin
        ml 0
        mt 20px
  &__speakers
    &-carover
      pos relative
      @media $lDesktopMedium
        mt 43px
  &__speaker
    pos relative
    w 100%
    pt 100%
    pos relative
    bgc white
    &-img
      pos absolute
      l 50%
      b 0
      max-width 100%
      max-height 100%
      transform translateX(-50%)
    &-content
      pos absolute
      bgc alpha(#0b3666, .6)
      t 0
      l 0
      w 100%
      h 100%
      padding 17%
      opacity 0
      transition .7s opacity
      @media $lDesktopMedium
        padding 16%
    &:hover &-content
      opacity 1
    &-name
      position relative
      t 20px
      opacity 0
      font-family $text
      fontify 25px 500 26px
      mb 20px
      mt -3px
      transition .7s top, .7s opacity
      @media $lDesktopMedium
        fontify 19px 500 22px
    &:hover &-name
      t 0
      opacity 1
      transition-delay .4s
    &-place
      position relative
      t 20px
      opacity 0
      font-style italic
      fontify 15px 400 25px
      transition .7s top, .7s opacity
      @media $lDesktopMedium
        fontify 12px 400 19px
    &:hover &-place
      t 0
      opacity 1
      transition-delay .4s
.videos
  bgc $blue
  pos relative
  z-index 1
  sp()
  pt 50px
  pb 258px
  @media $lDesktopMedium
    pt 50px
    pb 160px
  @media $LandscapeMedium
    pb 80px
  .common__title
    pos relative
    z-index 1
    max-width 600px
  &__carover
    mt 70px
    pos relative
    z-index 0
    @media $lDesktopMedium
      mt 36px
    .swiper-buttons
      t -139px
      @media $LandscapeMedium
        t 0
        mt 40px
    .swiper-container
      overflow visible
    .swiper-slide
      w calc((100vw - 320px) * .4 - 25px)
      @media $lDesktopMedium
        w calc((100vw - 80px) * .4 - 24px)
      &:hover
        & ^[0]__elem-img
          opacity 1
      @media $desktopMinFrom
        &_wide
          w calc((100vw - 320px) * .6 - 25px)
          @media $lDesktopMedium
            w calc((100vw - 80px) * .6 - 24px)
          &:hover
            & ^[0]__elem-img
              opacity .5
          & ^[0]__elem
            &-content
              max-width 660px
              padding 0
            &-name
              font-family $text
              fontify 57px 500 56px
              mb 32px
              @media $lDesktopMedium
                fontify 40px 500 40px
                mb 27px
            &-imgover
              pt 42%
              overflow visible
              bgc transparent
              @media $lDesktopMedium
                pt 45%
            &-img
              opacity .35
              z-index -1
              transform none
              l -160px
              t -115px
              w calc(100% + 420px)
              @media $lDesktopMedium
                l -40px
                t -71px
                w calc(100% + 287px)
            &-play
              mt 0
              ml 0
              b 30px
              l 0
              t auto
  &__elem
    &-imgover
      w 100%
      pt 68%
      pos relative
      overflow hidden
      bgc #0e3968
    &-img
      pos absolute
      imgCentering()
      opacity .75
      transition .7s opacity
    &-content
      padding 5% 8% 0
      @media $lDesktopMedium
        padding 8% 8% 0
    &-play
      pos absolute
      z-index 1
      t 50%
      l 50%
      size 66px
      mt -33px
      ml -33px
      bgc $pink
      border-radius 50%
      cursor pointer
      @media $lDesktopMedium
        size 50px
        mt -25px
        ml -25px
      &:before
        content ""
        pos absolute
        l 27px
        t 25px
        size 16px 18px
        background url("data:image/png;base64,") no-repeat center
        transition .7s background
        background-size cover
        @media $lDesktopMedium
          size 13px 14px
          l 20px
          t 18px
      &:hover:before
        background url("data:image/png;base64,") no-repeat center
    &-name
      font-family $text
      fontify 25px 500 26px
      mb 28px
      @media $lDesktopMedium
        fontify 19px 500 20px
        mb 16px
    &-text
      fontify 19px 400 30px
      @media $lDesktopMedium
        fontify 14px 400 24px
.partners
  pos relative
  z-index 1
  bgc white
  sp()
  pt 151px
  pb 96px
  @media $lDesktopMedium
    pt 150px
    pb 109px
  &__carover
    pos relative
    mt 69px
    @media $lDesktopMedium
      mt 48px
    .swiper-slide
      h auto
